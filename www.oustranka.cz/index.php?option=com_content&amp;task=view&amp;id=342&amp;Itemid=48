		<script type="text/javascript" language="Javascript">
		
		function buildLink(site, link)
		{
			var id = document.selectFeed.feeds.value;
			
			url = '/index.php?option=com_rd_rss&id=';
			url = site.concat(url);
			url = url.concat(id);
			url = url.concat(link);
			
			if(id==0)
			{
				alert("Vyberte zdroj RSS");	
			}
			else
			{
				 window.location=url;
			}			
		}
			
		</script>
		
